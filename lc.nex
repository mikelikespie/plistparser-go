package main
import ("fmt";"os")
type yySymType struct { l, c int }


func main() {
  v := new(yySymType)

  lex := NewLexer(os.Stdin)
  fmt.Printf("%d %d\n", v.l, v.c)

  lex.Lex(v)
}

